<p id="notice"><%= notice %></p>

<h1>Listing Modules</h1>

<table>
  <thead>
    <tr>
      <th>Program</th>
      <th>Module</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @modulecourses.each do |m| %>
      <tr>
         <% program = Program.find_by(id: m.program_id) %>
        <td><%= program.name %></td>
        <td><%= m.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>